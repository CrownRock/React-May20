// Resources
class Resources extends React.Component {
  constructor(props) {
    super(props);
    
    this.state = { resources: RESCOURCES };
  }
// }
    render() {
      const resourcesLinks = this.state.resources.map(resource => {
        return(
          <li key = {resource.title}>
            <RenderResource resource = {resource} />
              </li>
        );
      });
      // } 2
      return (
        <div>
          <h2>Additional Resources</h2>
          <ul>{resourcesLinks}</ul>
        </div>  
      );
   }
}
// end Resources

function RenderGame(props) {
  return (
    <strong>Game ID {props.game.id}: {props.game.name}</strong>
  );
}

// render Resources
function RenderResource({resource}) {
  return (
    <strong><a href = "{resource.url}" target = "_blank">{resource.title}</a></strong>
  );
}
